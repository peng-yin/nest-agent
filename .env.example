# Server
PORT=3000
NODE_ENV=development

# MySQL
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=nest_agent
MYSQL_PASSWORD=nest_agent_pass
MYSQL_DATABASE=nest_agent

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your-jwt-secret-change-in-production
JWT_EXPIRES_IN=7d

# Milvus
MILVUS_HOST=localhost
MILVUS_PORT=19530

# LLM Providers (configure the ones you need)
# Supports OpenAI-compatible APIs (e.g. SiliconFlow: https://api.siliconflow.cn/v1)
OPENAI_API_KEY=
OPENAI_BASE_URL=
ANTHROPIC_API_KEY=
DASHSCOPE_API_KEY=

# Default LLM
DEFAULT_LLM_PROVIDER=openai
DEFAULT_LLM_MODEL=gpt-4o

# Embedding model: BAAI/bge-m3 (1024 dim) via OpenAI-compatible API
# If using SiliconFlow, set OPENAI_BASE_URL above

# Tavily Search API (for web search tool)
TAVILY_API_KEY=
